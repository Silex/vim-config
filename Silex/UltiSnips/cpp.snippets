extends c

snippet cl "class .. (class)" !
class ${1:`!p snip.rv = snip.basename or "name"`}
{
  public:

	${1/(\w+).*/$1/}(${2:arguments});
	virtual ~${1/(\w+).*/$1/}();

  private:

	${0:// data}

};
endsnippet

snippet map "std::map" !
std::map<${1:key}, ${2:value}> ${3:m};$0
endsnippet

snippet vector "std::vector" !
std::vector<${1:char}> ${2:v};$0
endsnippet
